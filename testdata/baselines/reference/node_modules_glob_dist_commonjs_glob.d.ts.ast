        TypeReference
          Identifier: ' URL'
    PropertySignature
      Identifier: '
    /**
     * Include `.dot` files in normal matches and `globstar`
     * matches. Note that an explicit dot in a portion of the pattern
     * will always match dot files.
     */
    dot'
      QuestionToken
      BooleanKeyword
    PropertySignature
